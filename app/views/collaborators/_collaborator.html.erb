
<tr id='collaborator-<%= collaborator.user.id %>' class = "modal_stream_c">
  <%if collaborator.user.name.present?%>
    <td class = "pad_8"> <%= collaborator.user.name %></td>
  <%else%>  
    <td> <%= collaborator.user.email %></td>
  <%end%>
  <td id='permission-<%= collaborator.user.id %>' class = "custom_width pad_8">
    <%= collaborator.permission %>
  </td>
  <td>
    <a href="<%=repository_collaborator_path(:id => collaborator.user.id, :repository_id =>
    @repository.id)%>" data-method="delete" data-remote = "true">
      <span class="glyphicon glyphicon-remove-circle" id = "del_circ"></span>
    </a>
  </td>
</tr>
        