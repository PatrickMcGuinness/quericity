<%=render 'common/tabs'%>
<div class="tab-content padding-top40 tab-container">
   <a class="btn btn-primary float-right" href="<%= new_quiz_bank_path%>">Create Quiz</a>
  <div class="table-responsive  padding-top40">
    <%if @quiz_banks.present?%>
      <table class="table table-bordered">
        <tr>
          <th>Title </th>
          <th>Description </th>
          <th>Status</th>
        </tr>
        <%= render @quiz_banks %>
      </table>  
    <%else%>
      There is no quiz bank 
    <%end%>
  </div>
</div>