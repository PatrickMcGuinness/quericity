=render 'common/tabs'
.tab-content.padding-top40.tab-container
  .row
    .col-xs-3.left-panel
      = render partial: "analytics/left_panel"
    .col-xs-8.right-panel
      -if @served_quizzes.present?
        .col-xs-4.col-xs-offset-4
          %h3
            Percentages By Questions
        .col-xs-10.mar-top-10.col-xs-offset-1
          %img{src:"/assets/chart.gif",id:"up_logo_file2",style:"cursor:pointer;"}
        .col-xs-10.mar-top-20.col-xs-offset-1.analytics-table
          %table.table.table-striped
            %thead
              %th
                %span.glyphicon.glyphicon-text-width
                %span 
                  Title
              %th
                %span.glyphicon.glyphicon-calendar
                %span
                  Serve Data
                  
              %th
                %span.glyphicon.glyphicon-th-list
                %span
                  Average
            %tbody
              -@served_quizzes.each do |served_quiz|
                %tr
                  %td
                    = served_quiz.cloned_quiz_bank.title
                  %td
                    = get_date_and_time(served_quiz)
                  %td
                    = served_quiz.get_average
      -else
        %table.table
          %tbody
            %tr
              %td
                No Quiz Bank Served                                 

            