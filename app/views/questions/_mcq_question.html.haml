%tr.question{id: "question_#{@question.id}",data:{section_id: @question.section.id, id: @question.id}}
  %td.col-xs-11
    .col-xs-12
      .row
        .col-xs-11
          .row
            .col-xs-2.question-number
              %strong 
                = "Question No. #{@index}"
            .col-xs-9     
              =@question.description.html_safe
      .col-xs-10.options
        -@question.question_options.each do |option|
          .col-xs-8
            -if option.is_correct == true
              =check_box_tag :truefalse,nil, true,:class => "true-or-false", :disabled => true
              = option.answer
            -else
              =check_box_tag :truefalse,nil, false,:class => "true-or-false",:disabled => true
              = option.answer               